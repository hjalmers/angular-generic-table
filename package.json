{
  "name": "angular-generic-table",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build core --configuration production",
    "postbuild": "npx copyfiles projects/core/src/lib/scss/**/* dist/core/scss -f",
    "build:docs": "ng build docs --configuration production",
    "test": "ng test",
    "lint": "ng lint --fix --force",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "semantic-release": "semantic-release && npm run build",
    "format-all": "prettier --config ./.prettierrc --write \"projects/**/src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format-staged": "pretty-quick --staged",
    "format-check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "precommit": "npm run format-staged && npm run lint --fix --force",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run docs:storybook",
    "build-storybook": "ng run docs:build-storybook"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "pkgRoot": "./dist/core",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "release\\/([-a-z]*)",
        "channel": "${name.replace(/^release\\//g, '')}",
        "prerelease": "${name.replace(/^release\\//g, '')}"
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.1",
    "@angular/common": "^15.1.1",
    "@angular/compiler": "^15.1.1",
    "@angular/core": "^15.1.1",
    "@angular/forms": "^15.1.1",
    "@angular/platform-browser": "^15.1.1",
    "@angular/platform-browser-dynamic": "^15.1.1",
    "@angular/router": "^15.1.1",
    "bootstrap": "^5.1.3",
    "css-loader": "^6.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^6.6.7",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@a110/storybook-expand-all": "^1.1.1",
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "^15.1.2",
    "@angular/compiler-cli": "^15.1.1",
    "@angular/elements": "^15.1.1",
    "@babel/core": "^7.16.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@compodoc/compodoc": "^1.1.16",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/angular": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@types/jasmine": "^3.10.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "@webcomponents/custom-elements": "^1.5.0",
    "babel-loader": "^8.2.3",
    "codelyzer": "^6.0.2",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.1",
    "highlight.js": "^11.3.1",
    "husky": "^4.3.8",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "ng-packagr": "^15.1.1",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "replace": "^1.2.1",
    "semantic-release": "^19.0.5",
    "storybook-addon-themes": "^6.1.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.4"
  },
  "resolutions": {
    "webpack": "5.74.0"
  }
}
