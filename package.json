{
  "name": "angular-generic-table",
  "version": "4.14.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hjalmers/angular-generic-table.git"
  },
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "onpm ./@angular-generic-table/core install && onpm ./@angular-generic-table/column-settings install && ng serve",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "lint:ci": "npx ng lint --type-check --format verbose",
    "test": "ng test",
    "ci": "start-server-and-test start http-get://localhost:4200 cypress:run",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "core:compile:css": "node-sass --source-map true --include-path scss dist/@angular-generic-table/core/scss/*.scss dist/@angular-generic-table/core/css/generic-table-prebuild.css",
    "core:autoprefix:css": "postcss -u autoprefixer -r dist/@angular-generic-table/core/css/generic-table-prebuild.css",
    "core:minify:css": "cleancss -o dist/@angular-generic-table/core/css/generic-table-prebuild.min.css dist/@angular-generic-table/core/css/generic-table-prebuild.css --source-map",
    "core:rename:css": "renamer --find \"generic-table-prebuild\" --replace \"generic-table\" dist/@angular-generic-table/core/css/*",
    "core:build:css": "npm run core:compile:css && npm run core:autoprefix:css && npm run core:minify:css && npm run core:rename:css",
    "column-settings:compile:css": "node-sass --source-map true --include-path scss dist/@angular-generic-table/column-settings/scss/*.scss dist/@angular-generic-table/column-settings/css/column-settings-prebuild.css",
    "column-settings:autoprefix:css": "postcss -u autoprefixer -r dist/@angular-generic-table/column-settings/css/column-settings-prebuild.css",
    "column-settings:minify:css": "cleancss -o dist/@angular-generic-table/column-settings/css/column-settings-prebuild.min.css dist/@angular-generic-table/column-settings/css/column-settings-prebuild.css --source-map",
    "column-settings:rename:css": "renamer --find \"column-settings-prebuild\" --replace \"column-settings\" dist/@angular-generic-table/column-settings/css/*",
    "column-settings:build:css": "npm run column-settings:compile:css && npm run column-settings:autoprefix:css && npm run column-settings:minify:css && npm run column-settings:rename:css",
    "core:copy:scss": "copyfiles -f @angular-generic-table/core/generic-table.scss dist/@angular-generic-table/core/scss/",
    "column-settings:copy:scss": "copyfiles -f @angular-generic-table/column-settings/column-settings.scss dist/@angular-generic-table/column-settings/scss/",
    "core:copy:meta": "copyfiles -f LICENSE README.md CHANGELOG.md README.md dist/@angular-generic-table/core",
    "column-settings:copy:meta": "copyfiles -f LICENSE README.md CHANGELOG.md README.md dist/@angular-generic-table/column-settings",
    "build:core": "ng-packagr -p @angular-generic-table/core/ng-package.json && npm run core:copy:scss && npm run core:build:css",
    "build:column-settings": "ng-packagr -p @angular-generic-table/column-settings/ng-package.json && npm run column-settings:copy:scss && npm run column-settings:build:css",
    "prebuild:all": "npm run link-lib",
    "build:all": "rimraf dist/@angular-generic-table && npm run build:core  && copyfiles -u 1 dist/@angular-generic-table/core/**/* @angular-generic-table/column-settings/node_modules/ && npm run build:column-settings && npm run core:copy:meta && npm run column-settings:copy:meta",
    "publish:all": "npm publish dist/@angular-generic-table/core && npm publish dist/@angular-generic-table/column-settings",
    "link-lib": "cd @angular-generic-table/core && npm link && cd ../../ && npm link @angular-generic-table/core",
    "prettier": "prettier --list-different --write '**/*.{ts,json,scss,md}'",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "index.js",
  "dependencies": {
    "tether": "^1.4.3"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/language-service": "^5.2.10",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.1.1",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@types/jasmine": "2.5.38",
    "@types/marked": "^0.3.0",
    "@types/node": "^6.0.105",
    "@types/prismjs": "^1.9.0",
    "@types/tether": "^1.4.2",
    "angular-exemplify": "^2.0.1",
    "autoprefixer": "^7.2.6",
    "bootstrap": "^4.1.0",
    "clean-css-cli": "^4.1.11",
    "codelyzer": "^4.2.1",
    "copyfiles": "^1.0.0",
    "core-js": "^2.5.5",
    "cypress": "^2.1.0",
    "dragula": "^3.7.2",
    "jasmine-core": "2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "marked": "^0.3.19",
    "ng-packagr": "^3.0.0-rc.2",
    "ng2-dragula": "^1.5.0",
    "node-sass": "^4.8.3",
    "othernpm": "^1.1.0",
    "postcss-cli": "^4.1.0",
    "prettier": "1.12.1",
    "prismjs": "^1.14.0",
    "protractor": "~5.1.0",
    "raw-loader": "^0.5.1",
    "renamer": "^0.6.1",
    "rimraf": "^2.5.4",
    "rxjs": "^5.5.10",
    "start-server-and-test": "^1.4.1",
    "ts-helpers": "^1.1.1",
    "ts-node": "~2.0.0",
    "tsickle": "^0.27.2",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.4.2",
    "zone.js": "^0.8.26"
  }
}
