{
  "name": "angular-generic-table",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build --configuration production",
    "postbuild": "npx copyfiles projects/core/src/lib/scss/**/* dist/core/scss -f",
    "build:docs": "ng build docs --configuration production",
    "test": "ng test",
    "lint": "ng lint --fix --force",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "semantic-release": "semantic-release && npm run build",
    "format-all": "prettier --config ./.prettierrc --write \"projects/**/src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format-staged": "pretty-quick --staged",
    "format-check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "precommit": "npm run format-staged && npm run lint --fix --force",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run docs:storybook",
    "build-storybook": "ng run docs:build-storybook"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "pkgRoot": "./dist/core",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "release\\/([-a-z]*)",
        "channel": "${name.replace(/^release\\//g, '')}",
        "prerelease": "${name.replace(/^release\\//g, '')}"
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.3",
    "@angular/common": "~13.0.3",
    "@angular/compiler": "~13.0.3",
    "@angular/core": "~13.0.3",
    "@angular/forms": "~13.0.3",
    "@angular/platform-browser": "~13.0.3",
    "@angular/platform-browser-dynamic": "~13.0.3",
    "@angular/router": "~13.0.3",
    "bootstrap": "^5.1.3",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "~13.0.3",
    "@angular/elements": "^13.0.3",
    "@babel/core": "^7.16.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@compodoc/compodoc": "^1.1.16",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/angular": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@types/jasmine": "^3.10.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.20.40",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "@webcomponents/custom-elements": "^1.5.0",
    "babel-loader": "^8.2.3",
    "codelyzer": "^6.0.2",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.1",
    "highlight.js": "^11.3.1",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.1.2",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "replace": "^1.2.1",
    "semantic-release": "^18.0.1",
    "ts-node": "~8.3.0",
    "typescript": "~4.4.4"
  }
}
