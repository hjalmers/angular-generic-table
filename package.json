    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5",
    "zone.js": "^0.6.23"
  }
}
